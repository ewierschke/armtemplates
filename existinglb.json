{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "LoadbalancerName": {
      "type": "string"
    },
    "BackendPoolName": {
      "type": "string"
    },
    "existingLBResourceGroupName": {
      "type": "string",
      "metadata": {
        "description": "Subnet Name"
      }
    }
  },
  "variables": {},
  "resources": [],
  "outputs": {
    "LBRef": {
      "value": "[concat(resourceId(parameters('existingLBResourceGroupName'), 'Microsoft.Network/loadBalancers/', parameters('LoadbalancerName')), '/backendAddressPools/',parameters('BackendPoolName'))]",
      "type": "string"
    }
  }
}